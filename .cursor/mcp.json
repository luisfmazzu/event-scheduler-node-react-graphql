{
	"mcpServers": {
		"supabase": {
			"command": "npx",
			"args": [
				"-y",
				"@supabase/mcp-server-supabase@latest",
				"--read-only",
				"--project-ref=<project-ref>"
			],
			"env": {
				"SUPABASE_ACCESS_TOKEN": "<personal-access-token>"
			}
		},
		"context7": {
			"url": "https://mcp.context7.com/mcp"
		},
		"shadcn-ui-server": {
			"command": "npx",
			"args": ["@heilgar/shadcn-ui-mcp-server"]
		}
	}
}
